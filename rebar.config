{erl_opts, [debug_info]}.
{cover_enabled, true}.
{eunit_opts, [verbose, {report,{eunit_surefire,[{dir,"."}]}}]}.
{cover_enabled, true}.
{cover_export_enabled, true}.

{erl_opts,  [{parse_transform, lager_transform}, {parse_transform, stacktrace_transform}] }.

{deps_dir, "deps"}.
{deps, [
        {lager, ".*", {git, "https://github.com/erlang-lager/lager.git", {tag, "3.2.4"}}}
        {stacktrace_compat, ".*", {git, "https://github.com/g-andrade/stacktrace_compat.git", {tag, "1.0.0"}}}
]}.
